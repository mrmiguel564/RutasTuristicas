             <div class="w3-col w3-container" style="max-width:210px;max-height:300px;">
              <div class="w3-hide-small w3-card-4 w3-center">
                <div class="w3-hide-small w3-display-container w3-hover-opacity">
                  <img style="max-width:178px;max-height:200px" src="<%= datos[i].jpg %>">
                  <div class="w3-display-middle w3-display-hover">
                    <button class="w3-button w3-black" onclick="document.getElementById(<%=i%>).style.display='block'" >Comprar ahora <i class="fa fa-shopping-cart" ></i></button>
                     
                   
                  
                  </div> 
                  <div class=" w3-center">
                    <p><%= datos[i].nombre %><br>
                    <b><%= datos[i].precio%></b><br>              
                    <b><%= datos[i].contenido%></b></p>
                  </div>
                </div>
              </div>
            </div>
                
                    <div id=<%=i%> class="w3-modal">
                      <div class="w3-modal-content w3-animate-zoom" style="padding:32px">
                  
                        <div class="w3-container w3-white w3-center">
                          <i onclick="document.getElementById(<%=i%>).style.display='none'"
